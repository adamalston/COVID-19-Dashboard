{
    "name": "coviddashboard",
    "description": "COVID-19 Dashboard & Map",
    "version": "1.0.0",
    "author": "Adam Alston",
    "dependencies": {
        "axios": "^0.19.2",
        "gatsby": "^2.21.33",
        "gatsby-plugin-react-helmet": "^3.3.1",
        "gatsby-plugin-react-leaflet": "2.0.13",
        "gatsby-plugin-resolve-src": "^2.1.0",
        "gatsby-plugin-sass": "^2.3.1",
        "gatsby-source-filesystem": "^2.3.3",
        "leaflet": "^1.6.0",
        "node-sass": "^4.14.1",
        "path": "^0.12.7",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-helmet": "^6.0.0",
        "react-leaflet": "^2.7.0"
    },
    "devDependencies": {
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.0.1",
        "babel-preset-gatsby": "^0.4.2",
        "enzyme": "^3.11.0",
        "eslint": "^7.0.0",
        "eslint-config-zurgbot": "^0.0.7",
        "eslint-plugin-react": "^7.20.0",
        "eslint-plugin-react-hooks": "^4.0.2",
        "gh-pages": "^3.1.0",
        "husky": "^4.2.5",
        "jest": "^26.0.1",
        "lint-staged": "^10.2.2"
    },
    "keywords": [
        "gatsby",
        "sass",
        "leaflet",
        "mapping",
        "maps"
    ],
    "license": "MIT",
    "scripts": {
        "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
        "clean": "gatsby clean",
        "develop": "gatsby develop",
        "deploy": "gatsby build --prefix-paths && gh-pages -d public",
        "format": "prettier-eslint --eslint-config-path ./config/eslint.config.js --write $PWD/'**/*.js'",
        "lint": "eslint -c ./config/eslint.config.js . --ext .js",
        "start": "yarn develop",
        "serve": "gatsby serve",
        "test": "jest --config ./config/jest.config.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "**/*.js": [
            "yarn format",
            "yarn lint"
        ]
    },
    "homepage": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/adamalston/COVID-19-Dashboard-Map"
    },
    "bugs": {
        "url": "https://github.com/adamalston/COVID-19-Dashboard-Map/issues"
    }
}
